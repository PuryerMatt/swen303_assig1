<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1>Welcome to the Colenso Database!</h1>
        </div>
    </div>
</div>
<div class="container">

    <div class="well well-lg">
        <ul id="paths">

            {{#if direct}}
                {{#each pathArray}}
                    <a href="browse/?pathResult={{{this}}}" id="index-button" class="btn btn-info"  role="button">{{{this}}}</a>
                {{/each}}
            {{else}}
                {{#each pathArray}}
                    <a href="/?pathResult={{{this}}}" id="index-button" class="btn btn-info"  role="button">{{{this}}}</a>
                {{/each}}
            {{/if}}

        </ul>
    </div>

</div>





