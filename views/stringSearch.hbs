
<div class = "container">
    <h1>String Search</h1>
    <!--Action specifies where to submit the address, method is the method used when dubmitting the form (get or put)-->
    <form class="navbar-form navbar-left" role="search" action="" method ="get">
    <div class="form-group">
        <input name="searchInput" type="text" class="form-control" placeholder="Search">
    </div>
    <button  type="submit" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
    </form>
</div>
<div class = "container">
    <div class="well well-lg">
        <ul id="paths">
            {{#each queryResult}}
                <ul><a href="browse/?pathResult={{{this}}}">{{{this}}}</a></ul>
            {{/each}}
        </ul>
    </div>
</div>








